<?xml version="1.0" encoding="UTF-8"?>
<NoteArkivSettings>
	<persistenceProperties>
		<property key="jakarta.persistence.jdbc.driver">org.sqlite.JDBC</property>
		<property key="jakarta.persistence.jdbc.url">jdbc:sqlite:/notearkiv/test/database/Notearkiv.db</property>
		<property key="eclipselink.logging.logger">JavaLogger</property>
		<property key="eclipselink.logging.level">INFO</property>
		<property key="eclipselink.logging.parameters">true</property>
		<property key="eclipselink.logging.session">false</property>
	</persistenceProperties>
	<filesDirectory>/notearkiv/test/files</filesDirectory>
	<jsAppSettings>
		<backgroundImageUrl>./images/testbackground_small.png</backgroundImageUrl>
		<applicationTitle>TEST Tjølling sangforenings Notearkiv</applicationTitle>
		<applicationWelcomeTitle>Velkommen til notearkivet</applicationWelcomeTitle>
		<applicationWelcomeHtml>
			<![CDATA[<div>Dette er notearkivet til Tjølling sangforening</div>
			<div>&nbsp;</div>
			<div>Noter, stemmefiler og annen informasjon som finnes tilgjengelig i dette systemet eies av
				Tjølling Sangforening. Det er kun ment for Tjølling Sangforening sine medlemmer og må ikke
				distribueres videre.</div>]]>
		</applicationWelcomeHtml>
		<applicationId>NoteArkiv.tjollingTest</applicationId>
	</jsAppSettings>
	<chacheControlMaxAge>3600</chacheControlMaxAge>
</NoteArkivSettings>